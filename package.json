{
  "name": "k5e-cn",
  "version": "0.0.1",
  "private": true,
  "description": "A shadcn/ui-inspired component library for kintone customization",
  "keywords": ["kintone", "components", "cli", "shadcn"],
  "license": "MIT",
  "author": "k5e-cn contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/f4ah6o/k5e-cn.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "pnpm -r typecheck",
    "docs:dev": "pnpm --filter @k5e/www dev",
    "docs:build": "pnpm --filter @k5e/www build",
    "docs:serve": "pnpm --filter @k5e/www serve",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@changesets/cli": "^2.27.1",
    "typescript": "^5.5.4"
  }
}
